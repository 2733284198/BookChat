<view wx:for="{{menu}}" wx:key="index" wx:for-item="item" class='ul menu-tree font-lv3 color-semi'>
  <view class="li">
    <view class="navigator {{item.readed ? 'readed':''}} {{currentDocId == item.Id ? 'active':''}}" catchtap='itemClick' data-identify='{{item.book_id}}/{{item.id}}'>
      <text>{{item.title}}</text>
    </view>
    <menu-tree bind:itemClick="menuClick" menu="{{item.children}}" />
  </view>
</view>