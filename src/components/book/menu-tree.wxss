@import '../../app.wxss';

.menu-tree {
  line-height: 260%;
  overflow-x: hidden;
}

.ul {
  padding-left: 30rpx;
}

.li {
  width: 100%;
  box-sizing: border-box;
}

.li.active {
  color: #008cff;
}

.li navigator {
  padding-left: 40rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.readed navigator,.readed navigator:hover{
  color: #888;
  background: url('data:image/png;base64,') no-repeat 0rpx center;
}

.readed>navigator {
  color: #888;
}
